{
  "routes": [
    {
      "allowedRoles": ["anonymous", "authenticated"],
      "rewrite": "/.auth/login/aad",
      "route": "/login",
    },
    {
      "allowedRoles": ["anonymous", "authenticated"],
      "redirect": "/.auth/logout",
      "route": "/logout",
    },
    {
      "route": "/.auth/login/github",
      "statusCode": 404,
    },
    {
      "allowedRoles": ["employee"],
      "route": "/*",
    },
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/login",
      "statusCode": 302,
    },
  },
}
